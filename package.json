{
	"name": "dirtyswp",
	"displayName": "Dirty swp",
	"description": "Vim .swp file support",
	"version": "0.5.0",
	"publisher": "forsling",
	"engines": {
		"vscode": "^1.33.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "dirtyswp.start",
				"title": "Start Dirty .swp support"
			},
			{
				"command": "dirtyswp.stop",
				"title": "Stop Dirty .swp support"
			}
		]
	},
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"eslint": "^5.13.0",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	}
}
