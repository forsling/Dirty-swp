{
	"name": "dirtyswp",
	"displayName": "Dirty swp",
	"description": "Vim .swp file support",
	"version": "0.6.2",
	"publisher": "forsling",
	"engines": {
		"vscode": "^1.33.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "dirtyswp.start",
				"title": "Dirty swp: Start/resume"
			},
			{
				"command": "dirtyswp.stop",
				"title": "Dirty swp: Pause"
			},
			{
				"command": "dirtyswp.listswp",
				"title": "Dirty swp: List locked files"
			},
			{
				"command": "dirtyswp.forcelock",
				"title": "Dirty swp: Lock file until close"
			}
		],
		"configuration": {
			"type": "object",
			"title": "Dirty .swp",
			"properties": {
				"dirtyswp.startActive": {
					"type": "boolean",
					"default": true,
					"description": "Whether or not Dirty .swp starts with active locking/monitoring of files"
				}
			}
		}
	},
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"eslint": "^5.13.0",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	}
}
